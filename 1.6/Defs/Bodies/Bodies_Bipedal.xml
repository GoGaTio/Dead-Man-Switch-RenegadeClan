<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Bipedal with head -->
  <BodyDef>
    <defName>DMSRC_Bipedal</defName>
    <label>bipedal</label>
    <corePart>
      <def>DMS_HeavyChassis</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>MechanicalHead</def>
          <height>Top</height>
          <groups>
            <li>HeadAttackTool</li>
          </groups>
          <coverage>0.05</coverage>
          <parts>
            <li>
              <def>ArtificialBrain</def>
              <depth>Inside</depth>
              <coverage>0.05</coverage>
            </li>
            <li>
              <def>DMS_CompactSenser</def>
              <coverage>0.05</coverage>
            </li>
            <li>
              <def>SightSensor</def>
              <coverage>0.05</coverage>
            </li>
            <li>
              <def>HearingSensor</def>
              <coverage>0.05</coverage>
            </li>
            <li>
              <def>SmellSensor</def>
              <coverage>0.05</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>left weapon mount</customLabel>
          <coverage>0.1</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>left vertical rotor</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>MechanicalHand</def>
                  <customLabel>left horizontal rotor</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <parts>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>left ammo feeder</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>left trigger safty</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>left mount lock</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>right weapon mount</customLabel>
          <coverage>0.1</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>right vertical rotor</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>MechanicalHand</def>
                  <customLabel>right horizontal rotor</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <parts>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>right ammo feeder</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>right trigger safty</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>RightHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>MechanicalFinger</def>
                      <customLabel>right mount lock</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>RightHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.1</coverage>
          <height>Bottom</height>
          <groups>
              <li>LeftMechanicalLeg</li>
          </groups>
          <parts>
            <li>
              <def>DMS_ArmourPlate</def>
              <customLabel>leg armour plate</customLabel>
              <coverage>0.3</coverage>
              <depth>Outside</depth>
            </li>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>left foot</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.1</coverage>
          <height>Bottom</height>
          <groups>
            <li>RightMechanicalLeg</li>
          </groups>
          <parts>
            <li>
              <def>DMS_ArmourPlate</def>
              <customLabel>leg armour plate</customLabel>
              <coverage>0.3</coverage>
              <depth>Outside</depth>
            </li>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>right foot</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>DMSRC_ModuleMount</def>
          <customLabel>left module mount</customLabel>
          <coverage>0.05</coverage>
          <depth>Outside</depth>
        </li>
        <li>
          <def>DMSRC_ModuleMount</def>
          <customLabel>right module mount</customLabel>
          <coverage>0.05</coverage>
          <depth>Outside</depth>
        </li>
        <li>
          <def>DMS_ArmourPlate</def>
          <customLabel>waist armour plate</customLabel>
          <coverage>0.1</coverage>
          <depth>Outside</depth>
        </li>
        <li>
          <def>DMS_ArmourPlate</def>
          <customLabel>chest armour plate</customLabel>
          <coverage>0.1</coverage>
          <depth>Outside</depth>
        </li>
        <li>
          <def>DMS_Battery</def>
          <customLabel>main battery array</customLabel>
          <coverage>0.02</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>DMS_Battery</def>
          <customLabel>secondary battery array</customLabel>
          <coverage>0.02</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>FluidReprocessor</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>FluidReprocessor</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
      </parts>
    </corePart>
  </BodyDef>

