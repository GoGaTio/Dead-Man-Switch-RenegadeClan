<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingSetMakerDef>
    <defName>DMSRC_SecurityContainer_NaraRed</defName>
    <root Class="ThingSetMaker_Sum">
      <options>

        <!-- Prosthetics -->
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <qualityGenerator>Reward</qualityGenerator> <!-- Just in case -->
              <filter>
                <thingDefs>
                  <li>PowerClaw</li>
                  <li>Joywire</li>
                  <li>Painstopper</li>
                  <li>DMS_NutrientPort</li>
                  <li>DMS_ProstheticLeg</li>
                  <li>DMS_ProstheticArm</li>
                  <li>DMS_ProstheticEye</li>
                  <li>DMS_ProstheticKidney</li>
                  <li>DMS_ProstheticLung</li>
                  <li>DMS_ProstheticSpine</li>
                </thingDefs>
              </filter>
              <countRange>2~5</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.9</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <qualityGenerator>Reward</qualityGenerator> <!-- Just in case -->
              <filter>
                <tradeTagsToAllow>
                    <li>DMSRC_Neural</li>
                  </tradeTagsToAllow>
<countRange>1~3</countRange> 
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Techprints -->
        <!-- note this is intended to ignore player needs entirely, it gives entirely random techprints -->
        <li>
          <chance>1</chance> 
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingSetMakerTagsToAllow>
                  <li>Techprint</li>
                </thingSetMakerTagsToAllow>
              </filter>
              <countRange>1~3</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Barrels -->
        <li>
          <chance>0.85</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ReinforcedBarrel</li>
                </thingDefs>
              </filter>
              <countRange>1~5</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Components -->
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentIndustrial</li>
                </thingDefs>
              </filter>
              <countRange>10~20</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentStamped</li>
                </thingDefs>
              </filter>
              <countRange>5~10</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
<li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentSpacer</li>
                </thingDefs>
              </filter>
              <countRange>2~7</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Neurocomputer</li>
                </thingDefs>
              </filter>
              <countRange>1~3</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
<li>
          <chance>0.75</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentControlChip</li>
                </thingDefs>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
<li>
          <chance>0.25</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentFireControl</li>
                </thingDefs>
              </filter>
<countRange>1~1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentTransmitter</li>
                </thingDefs>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <chance>0.5</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>DMSRC_ComponentGBEP</li>
                </thingDefs>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

<li>
          <chance>0.35</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Artifuscle</li>
                </thingDefs>
              </filter>
              <countRange>30~45</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <!-- Boss caller -->
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>DMS_EncryptedTransmitter</li>
<li MayRequire="Aoba.DeadManSwitch.AncientCorps" >DMSAC_RecoverTransmitter</li>
                </thingDefs>
              </filter>
              <countRange>1~3</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Survival meals -->
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>MealSurvivalPack</li>
                </thingDefs>
              </filter>
              <countRange>10~25</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>


        <!-- Meds -->
        <li>
          <chance>1</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>MedicineIndustrial</li>
                </thingDefs>
              </filter>
              <countRange>2~8</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>


        <li>
          <chance>0.2</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>MedicineUltratech</li>
                </thingDefs>
              </filter>
              <countRange>1~3</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>


</Defs>
