    <description>Produce assault armor and variants, developed for direct combat.</description>
    <baseCost>10000</baseCost>
    <techLevel>Industrial</techLevel>
    <prerequisites>
      <li>DMS_EquipmentElite</li>
      <li>AdvancedFabrication</li>
    </prerequisites>
    <researchViewX>3.00</researchViewX>
    <researchViewY>5.00</researchViewY>
  </ResearchProjectDef>

  <ResearchProjectDef ParentName="DMS_BaseTech">
    <defName>DMSRC_FSRMunitions</defName>
    <label>FSR munition</label>
    <description>Allows to produce ammo, that creates spark streams and flame blast at the impact position.</description>
    <baseCost>1000</baseCost>
    <techLevel>Industrial</techLevel>
    <prerequisites>
      <li>MultiAnalyzer</li>
    </prerequisites>
    <researchViewX>1.00</researchViewX>
    <researchViewY>1.90</researchViewY>
  </ResearchProjectDef>

  <ResearchProjectDef ParentName="DMS_BaseTech">
    <defName>DMSRC_BeamFocusing</defName>
    <label>beam focusing</label>
